{
    "manifest_version": 3,
    "name": "Refund Scam Blocker",
    "description":"A scam warner that will populate potential suspicious websites with warning screens",
    "version": "1.2",
    "permissions": ["scripting", "webNavigation"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "16_logo.png",
      "48": "48_logo.png",
      "128": "128_logo.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["./styles/popup.css"],
        "matches": ["<all_urls>"]
      }
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "run_at": "document_start"
      }
    ],
    "host_permissions": [
      "https://anydesk.com/*",
      "https://www.teamviewer.com/*",
      "https://www.logmein.com/*"
    ]
  }
  